-- üßæ Tab Info (Th√™m note)
local InfoTab = Window:CreateTab("Info")

local playerCount = #game.Players:GetPlayers()
local maxPlayers = game.Players.MaxPlayers

InfoTab:CreateParagraph({
    Title = "User & Game Info",
    Content = "Username: " .. game.Players.LocalPlayer.Name ..
              "\nJobID: " .. tostring(game.JobId) ..
              "\nGameID: " .. tostring(game.PlaceId) ..
              "\nPlayers: " .. playerCount .. " / " .. maxPlayers ..
              "\nYT: GM1nhRBL" ..
              "\nDiscord: https://discord.gg/pq7jgm2gwg"
})

InfoTab:CreateButton({
    Name = "Copy link Discord",
    Callback = function()
        local info = "https://discord.gg/pq7jgm2gwg"
        setclipboard(info)
        Rayfield:Notify({Title = "Copied!", Content = "Link Discord copied!", Duration = 3, Callback = function() end})
    end,
})


InfoTab:CreateButton({
    Name = "Copy link YT",
    Callback = function()
        local info = "https://www.youtube.com/@NgGMinh2807"
        setclipboard(info)
        Rayfield:Notify({Title = "Copied!", Content = "Link YT copied!", Duration = 3, Callback = function() end})
    end,
})

InfoTab:CreateButton({
    Name = "Copy JobID",
    Callback = function()
        if setclipboard then
            setclipboard(game.JobId)
            Rayfield:Notify({
                Title = "‚úÖ JobID ƒë√£ ƒë∆∞·ª£c sao ch√©p!",
                Content = "JobID c·ªßa server hi·ªán t·∫°i ƒë√£ ƒë∆∞·ª£c copy v√†o clipboard.",
                Duration = 3,
            })
        else
            Rayfield:Notify({
                Title = "‚ö†Ô∏è Kh√¥ng th·ªÉ sao ch√©p!",
                Content = "Executor c·ªßa b·∫°n kh√¥ng h·ªó tr·ª£ setclipboard.",
                Duration = 3,
            })
            print("JobID:", game.JobId)
        end
    end,
})

local TeleportService = game:GetService("TeleportService")

local jobBox = InfoTab:CreateInput({
    Name = "Join JobID",
    PlaceholderText = "Enter JobID to tp",
    RemoveTextAfterFocusLost = false,
    Callback = function(Value)
        getgenv().targetJobID = Value
    end,
})

InfoTab:CreateButton({
    Name = "Join Server",
    Callback = function()
        if getgenv().targetJobID and getgenv().targetJobID ~= "" then
            Rayfield:Notify({
                Title = "ƒêang teleport...",
                Content = "ƒêang tham gia server c√≥ JobID: " .. getgenv().targetJobID,
                Duration = 3,
            })
            TeleportService:TeleportToPlaceInstance(game.PlaceId, getgenv().targetJobID, game.Players.LocalPlayer)
        else
            Rayfield:Notify({
                Title = "L·ªói",
                Content = "Vui l√≤ng nh·∫≠p JobID tr∆∞·ªõc khi join!",
                Duration = 3,
            })
        end
    end,
})

