local TargetName = ""

MainTab:CreateInput({
    Name = "Target Username",
    PlaceholderText = "Enter Username...",
    RemoveTextAfterFocusLost = false,
    Callback = function(text)
        TargetName = text
    end
})

MainTab:CreateButton({
    Name = "Run Stand Script",
    Callback = function()
        if TargetName == "" then
            Rayfield:Notify({
                Title = "Failed ‚ùå",
                Content = "You not enter username",
                Duration = 4
            })
            return
        end

        getgenv().TargetUsername = TargetName

        Rayfield:Notify({
            Title = "Running...",
            Content = "Set TargetUsername = " .. TargetName,
            Duration = 4
        })

        loadstring(game:HttpGet("https://raw.githubusercontent.com/Dragonfly5101/Minosr/refs/heads/main/Stand"))()
    end
})

